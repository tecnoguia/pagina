	<script>
		$(document).ready(function() {
			$('a[href^="#"]').on('click', function(e) {
				e.preventDefault();

				var target = this.hash,
				    $target = $(target);

				$('html, body').stop().animate({
					'scrollTop' : $target.offset().top
				}, 900, 'swing', function() {
					window.location.hash = target;
				});
			});
		});
	</script>

	<script>
		$(document).ready(function() {

			$(".btn-promo").click(function(e) {
				e.preventDefault();
				$(".promo").collapse('show');

				$("body, html").animate({
					scrollTop : $($(this).attr('href')).offset().top
				}, 600);
			});
			$(".btn-galeria").click(function(e) {
				e.preventDefault();
				$(".galeria").collapse('show');

				$("body, html").animate({
					scrollTop : $($(this).attr('href')).offset().top
				}, 600);
			});
			$(".btn-proximos").click(function(e) {
				e.preventDefault();
				$(".proximos").collapse('show');

				$("body, html").animate({
					scrollTop : $($(this).attr('href')).offset().top
				}, 600);
			});
			$(".btn-anteriores").click(function(e) {
				e.preventDefault();
				$(".anteriores").collapse('show');

				$("body, html").animate({
					scrollTop : $($(this).attr('href')).offset().top
				}, 600);
			});

		});
	</script>
